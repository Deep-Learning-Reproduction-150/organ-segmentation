{
    "name": "organ_paper_reproduction",
    "resume": true,
    "preload": true,
    "wandb_project_name": "organ paper segmentation",
    "wandb_prediction_examples": 8,
    "wandb_api_key": "<your wandb api key>",
    "tensorboard": null,
    "model": {
        "name": "OrganNet25D",
        "input_shape": [
            2,
            10,
            48,
            256,
            256
        ],
        "hdc_dilations": [
            1,
            2,
            5,
            9
        ]
    },
    "training": {
        "epochs": 300,
        "detect_bad_gradients": false,
        "grad_norm_clip": 1,
        "loss": {
            "name": "CombinedLoss",
            "alpha": [
                1.0,
                4.0,
                1.0,
                4.0,
                4.0,
                1.0,
                1.0,
                3.0,
                3.0,
                0.5
            ],
            "input_dim": [
                48,
                256,
                256
            ]
        },
        "split_ratio": 0.77,
        "batch_size": 2,
        "num_workers": 0,
        "evaluate": true,
        "optimizer": {
            "name": "Adam",
            "learning_rate": 0.001,
            "betas": [
                0.9,
                0.999
            ]
        },
        "lr_scheduler": {
            "name": "MultiStepLR"
        },
        "dataset": {
            "root": "./data/train",
            "remove_false_samples": true,
            "labels": [
                "BrainStem",
                "Chiasm",
                "Mandible",
                "OpticNerve_L",
                "OpticNerve_R",
                "Parotid_L",
                "Parotid_R",
                "Submandibular_L",
                "Submandibular_R"
            ],
            "transform": [
                {
                    "name": "Transpose",
                    "dim_1": 0,
                    "dim_2": -1
                },
                {
                    "name": "EasyResize",
                    "width": 256,
                    "height": 256,
                    "depth": 48
                },
                {
                    "name": "StandardScaleTensor"
                }
            ]
        }
    },
    "evaluation": {
        "name": "ChenEvaluator"
    }
}