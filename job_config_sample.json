{
    "name": "paper_reproduction",
    "resume": true,
    "preload": false,
    "wandb_project_name": "organ segmentation",
    "wandb_api_key": "<your wandb api key>",
    "tensorboard": null,
    "model": {
        "reset": true,
        "input_shape": [48, 256, 256]
    },
    "training": {
        "epochs": 4,
        "loss": {
            "name": "MSELoss",
            "parameter_a": 2
        },
        "split_ratio": 0.9,
        "wandb_id": null,
        "batch_size": 2,
        "num_workers": 0,
        "evaluate": true,
        "optimizer": {
            "name": "Adam",
            "learning_rate": 0.001,
            "betas": [0.9, 0.999]
        },
        "lr_scheduler": {
            "name": "LinearLR",
            "start_factor": 1,
            "end_factor": 0.01,
            "total_iters": 100
        },
        "dataset": {
            "root": "./data",
            "remove_false_samples": true,
            "transform": [
                {
                    "name": "Transpose",
                    "dim_1": 0,
                    "dim_2": -1
                },
                {
                    "name": "EasyResize",
                    "width": 256,
                    "height": 256,
                    "depth": 48
                },
                {
                    "name": "Normalize",
                    "mean": 0,
                    "std": 1
                }
            ]
        }
    },
    "evaluation": {
        "name": "ChenEvaluator"
    }
}