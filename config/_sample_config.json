{
    "name": "testing_organ_segmentation",
    "model": {
        "reset": true
    },
    "training": {
        "epochs": 4,
        "loss": "MSELoss",
        "split_ratio": null,
        "wandb_id": null,
        "batch_size": 4,
        "num_workers": 0,
        "evaluate": true,
        "optimizer": {
            "name": "Adam",
            "learning_rate": 0.001,
            "betas": [0.9, 0.999]
        },
        "dataset": {
            "root": "./data",
            "preload": false,
            "remove_false_samples": true,
            "transform": [
                {
                    "name": "Resize",
                    "width": 100,
                    "height": 100,
                    "depth": 50
                },
                {
                    "name": "Normalize",
                    "mean": 0,
                    "std": 1
                }
            ]
        }
    },
    "evaluation": {
        "evaluator": "DiceEvaluator"
    }
}