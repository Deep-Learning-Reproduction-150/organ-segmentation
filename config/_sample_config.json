{
    "name": "testing_organ_segmentation",
    "resume": true,
    "model": {
        "reset": true
    },
    "training": {
        "epochs": 4,
        "loss": {
            "name": "DiceLoss",
            "parameter_a": 2
        },
        "split_ratio": null,
        "wandb_id": null,
        "batch_size": 2,
        "num_workers": 0,
        "evaluate": true,
        "optimizer": {
            "name": "Adam",
            "learning_rate": 0.001,
            "betas": [0.9, 0.999]
        },
        "dataset": {
            "root": "./data",
            "preload": false,
            "remove_false_samples": true,
            "transform": [
                {
                    "name": "Resize",
                    "width": 256,
                    "height": 256,
                    "depth": 48
                },
                {
                    "name": "Reshape",
                    "width": 256,
                    "height": 256,
                    "depth": 48
                },
                {
                    "name": "Normalize",
                    "mean": 0,
                    "std": 1
                }
            ]
        }
    },
    "evaluation": {
        "evaluator": "DiceEvaluator"
    }
}