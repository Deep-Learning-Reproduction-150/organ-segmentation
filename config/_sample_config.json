{
    "name": "organ_segmentation",
    "model": {
        "reset": true
    },
    "training": {
        "epochs": 4,
        "loss": "MSELoss",
        "split_ratio": 0.8,
        "batch_size": 16,
        "num_workers": 0,
        "evaluate": true,
        "optimizer": {
            "name": "adam",
            "learning_rate": 0.001,
            "betas": [
                0.9,
                0.999
            ]
        },
        "dataset": {
            "root": "./data",
            "preload": true,
            "transform": [
                {
                    "name": "ShiftToZero"
                },
                {
                    "name": "Padding",
                    "height": 500,
                    "width": 500,
                    "depth": 100
                },
                {
                    "name": "Normalize",
                    "params": [
                        0,
                        1
                    ]
                },
                {
                    "name": "EliminateNans"
                },
                {
                    "name": "Rescale"
                }
            ]
        }
    },
    "evaluation": false,
    "inference": false,
}