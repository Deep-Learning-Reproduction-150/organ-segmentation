{
    "name": "testing_organ_segmentation",
    "model": {
        "reset": true,
        "input_shape": [32, 64, 64]
    },
    "training": {
        "epochs": 4,
        "loss": {
            "name": "MSELoss",
            "parameter_a": 2
        },
        "split_ratio": null,
        "wandb_id": null,
        "batch_size": 1,
        "num_workers": 0,
        "evaluate": true,
        "optimizer": {
            "name": "Adam",
            "learning_rate": 0.001,
            "betas": [0.9, 0.999]
        },
        "dataset": {
            "root": "./data",
            "preload": true,
            "remove_false_samples": true,
            "transform": [
                {
                    "name": "EasyResize",
                    "width": 64,
                    "height": 64,
                    "depth": 32
                }
            ]
        }
    },
    "evaluation": {
        "evaluator": "DiceEvaluator"
    }
}